# Sui Tipping System - Environment Variables
# Copy this file to .env and fill in your contract IDs

# Your deployed contract package ID
VITE_TIPPING_PACKAGE_ID=0x...

# Your deployed TippingSystem shared object ID
VITE_TIPPING_SYSTEM_ID=0x...

# Default network (optional, defaults to 'mainnet')
# Options: 'localnet' | 'devnet' | 'testnet' | 'mainnet'
VITE_DEFAULT_NETWORK=mainnet

